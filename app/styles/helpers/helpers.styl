for m in 0 10 20 30 40 50 60 70 80 100
  .m_top-{m}
    margin-top unit(m,'px') !important

for m in 0 10 20 30 40 50 60 70 80 100
  .m_bottom-{m}
    margin-bottom unit(m,'px') !important

for p in 0 10 20 30 40 50 60 70 80 100
  .p_top-{p}
    padding-top unit(p,'px') !important

for p in 0 10 20 30 40 50 60 70 80 100
  .p_bottom-{p}
    padding-bottom unit(p,'px') !important

@import "variables"
@import "alias"
@import "mixins"
@import "fonts"


