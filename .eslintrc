{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "spread" : true,
    "restParams" : true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery" : true
  },
  "rules": {
    "no-console": 0,
    "require-yield": 0,
    "no-constant-condition": ["error", { "checkLoops": false }],
    "no-unused-vars" : 2,
    "no-undef" : 2
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "indent": 2
}
