.body.loading {
    overflow: hidden;
}

.body.loading > .page{
    visibility: hidden;
}
.body.loading > footer{
    visibility: hidden;
}


.progressjs-inner {
    width: 0;
}
.progressjs-progress {
    z-index: 9999999;
}

/* blue theme with overlay layer, no percent bar */
.progressjs-theme-blueOverlay {
    background: #e8e7e4;
    -webkit-transition: all 0.2s ease-out;
    -moz-transition: all 0.2s ease-out;
    -o-transition: all 0.2s ease-out;
    transition: all 0.2s ease-out;
    height: 100%;
}
.progressjs-theme-blueOverlay .progressjs-inner {
    background: transparent url('data:image/png;base64,') 0 0 no-repeat;
    height: 130px;
    width: 100% !important;
    max-width: 410px;
    position: absolute;
    top:50%;
    left:50%;
    margin-left: -205px;
    margin-top: -65px;
    -webkit-transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
    -o-transition: all 0.3s ease-out;
    transition: all 0.3s ease-out;

}
.progressjs-theme-blueOverlay .progressjs-inner:before {
    background: transparent url('data:image/png;base64,') 110px 0 no-repeat;
    height: 93px;
    position: absolute;
    top:0;
    left:0;
    right: 0;
    -webkit-transition: all 1s ease-out;
    -moz-transition: all 1s ease-out;
    -o-transition: all 1s ease-out;
    transition: all 1s ease-out;
    opacity: 0;
    content: '';
}
.progressjs-theme-blueOverlay .progressjs-inner.l100pr:before {
    opacity: 1;
}

.progressjs-theme-blueOverlay.progressjs-end {
    opacity: 0 !important;
}
.progressjs-theme-blueOverlay .progressjs-percent {
    display: block;
    margin-top: 110px;
    font-size: 18px;
    width: 93px;
    text-align: center;

}
